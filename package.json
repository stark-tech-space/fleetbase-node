{
  "name": "@stark-tech-space/fleetbase-node",
  "version": "0.1.2",
  "description": "Fleetbase node API package",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "files": [
    "lib/"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json && tsc -p tsconfig-cjs.json",
    "prepublishOnly": "npm run build",
    "test": "npx jest"
